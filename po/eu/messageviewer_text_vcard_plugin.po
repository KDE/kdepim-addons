#: text_vcard.cpp:223
#, kde-format
msgid "View Business Card"
msgstr "Ikusi bisita-txartela"

#: text_vcard.cpp:224
#, kde-format
msgid "Save Business Card As…"
msgstr "Gorde bisita-txartela honela..."

#: text_vcard.cpp:241
#, kde-format
msgid "Add this contact to the address book."
msgstr "Gehitu kontaktu hau helbide-liburuari."

#: text_vcard.cpp:241
#, kde-format
msgid "Update this contact to the address book."
msgstr "Eguneratu kontaktu hau helbide-liburuan."

#: text_vcard.cpp:243
#, kde-format
msgid "Add \"%1\" to the address book."
msgstr "Gehitu \"%1\" helbide-liburuan."

