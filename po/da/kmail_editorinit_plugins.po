#: externalcomposer/externalcomposerconfiguredialog.cpp:14
#, kde-format
msgctxt "@title:window"
msgid "Configure \"External Composer\" Plugin"
msgstr "Indstil plugin for \"ekstern brevskriver\""

#: externalcomposer/externalcomposerconfigurewidget.cpp:21
#, kde-format
msgid "Use external editor instead of composer"
msgstr "Brug ekstern editor i stedet for brevskriveren"

#: externalcomposer/externalcomposerconfigurewidget.cpp:48
#, kde-format
msgid ""
"<b>%f</b> will be replaced with the filename to edit.<br /><b>%w</b> will be "
"replaced with the window id.<br /><b>%l</b> will be replaced with the line "
"number."
msgstr ""
"<b>%f</b> vil blive erstattet med filnavnet der skal redigeres.<br /><b>%w</"
"b> vil blive erstattet med vindues-id'et.<br /><b>%l</b> vil blive erstattet "
"med linjenummeret."
