#: text_vcard.cpp:130
#, kde-format
msgid "[Add this contact to the address book]"
msgstr "[एतत् सम्पर्कं पतापुस्तके योजयन्तु]।"

#: text_vcard.cpp:136
#, kde-format
msgid "[Update this contact in the address book]"
msgstr "[एतत् सम्पर्कं पतापुस्तके अद्यतनं कुर्वन्तु]।"

#: text_vcard.cpp:141
#, kde-format
msgid "[This contact is already in addressbook]"
msgstr "[एषः सम्पर्कः सम्बोधनपुस्तिकायां पूर्वमेव अस्ति]।"

#: text_vcard.cpp:215
#, kde-format
msgid "View Business Card"
msgstr "व्यवसायपत्रं पश्यन्तु"

#: text_vcard.cpp:216
#, kde-format
msgid "Save Business Card As…"
msgstr "व्यवसायपत्रं रक्षतु यथा..."

#: text_vcard.cpp:233
#, kde-format
msgid "Add this contact to the address book."
msgstr "एतत् सम्पर्कं पतापुस्तके योजयन्तु।"

#: text_vcard.cpp:233
#, kde-format
msgid "Update this contact to the address book."
msgstr "एतत् सम्पर्कं पतापुस्तके अद्यतनं कुर्वन्तु।"

#: text_vcard.cpp:235
#, kde-format
msgid "Add \"%1\" to the address book."
msgstr "पतापुस्तके \"%1\" योजयन्तु ।"

#: text_vcard.cpp:235
#, kde-format
msgid "Update \"%1\" to the address book."
msgstr "\"%1\" इत्येतत् पतापुस्तके अद्यतनं कुर्वन्तु ।"

#: text_vcard.cpp:259
#, kde-format
