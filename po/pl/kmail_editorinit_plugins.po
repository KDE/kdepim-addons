#: externalcomposer/externalcomposerconfiguredialog.cpp:14
#, kde-format
msgctxt "@title:window"
msgid "Configure \"External Composer\" Plugin"
msgstr "Ustawienia wtyczki \"Zewnętrznego edytora\""

#: externalcomposer/externalcomposerconfigurewidget.cpp:23
#, kde-format
msgctxt "@option:check"
msgid "Use external editor instead of composer"
msgstr "Użyj edytora zewnętrznego zamiast wbudowanego"

#: externalcomposer/externalcomposerconfigurewidget.cpp:52
#, kde-format
msgctxt "@label:textbox"
msgid ""
"<b>%f</b> will be replaced with the filename to edit.<br /><b>%w</b> will be "
"replaced with the window id.<br /><b>%l</b> will be replaced with the line "
"number."
msgstr ""
"<b>%f</b> zostanie zastąpione nazwą pliku do edycji.<br /><b>%w</b> zostanie "
"zastąpione id okna.<br /><b>%l</b> zostanie zastąpione numerem wiersza."
